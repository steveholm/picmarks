<div class="container_fluid">
  <div class="row-fluid">
    <h1>My Picmarks</h1>
  </div>
  
  <div class="row-fluid">
    <h5><%= @count %> Photos</h5>
  </div>
  

<ul>
  
  <% @pictures.each do |p| %>
    <li class="span3 my_photos"><img src="<%= p.url %>">
      <p><%= p.title %></p>
    </li>
  <% end %>

</ul>

</div>
